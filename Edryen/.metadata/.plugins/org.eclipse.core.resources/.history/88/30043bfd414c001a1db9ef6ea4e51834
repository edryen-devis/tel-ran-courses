#include<stdio.h>
void printHello(int num);
void printH(int num);

int main(){
	printHello(5);
	printH(10);
	return 0;
}
void printHello(int num){
	int i;
	for (i=0; i<=num; i++){
		printf("%d Hello!\n", i);

	}
}
void printH(int num){
	int i=0;
	do{
			printf(" %d Hello!\n", i);
			i++;

		}while(i<=num);
}
