#include<stdio.h>

void printStars(int starsInRow, int rows);

int main(){
	printStars(5,7);

	return 0;
}

void printStars(int starsInRow, int rows){
	int i, j;

	for(i=1; i<=rows; i++){
		for(i=1; i<=starsInRow; i++){
			printf("* ");
		}
		printf("\n");
	}
}
