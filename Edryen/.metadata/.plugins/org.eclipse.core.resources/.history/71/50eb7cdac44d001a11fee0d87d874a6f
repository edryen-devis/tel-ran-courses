#include<stdio.h>
void printEvenNumber();

int main(){
	printEvenNumber();
	return 0;

}

void printEvenNumber(int num){
	//int i;
	if(num%2==0){

	for(num; num>0; num=num-2){
		printf("%d\n", num);
	}
	else{
		num=num-1;
	}
	}
}


